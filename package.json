{
  "name": "commercify",
  "version": "1.0.0",
  "description": "MERN E commerce website with Shopping cart",
  "main": "server/server.js",
  "type": "module",
  "scripts": {
    "server": "nodemon /server/server.js",
    "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "seed": "cd server && npm run seed",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/breakfireaus/commercify.git"
  },
  "author": "Breakfireaus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/breakfireaus/commercify/issues"
  },
  "homepage": "https://github.com/breakfireaus/commercify#readme",
  "dependencies": {
    "concurrently": "^7.6.0"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}
